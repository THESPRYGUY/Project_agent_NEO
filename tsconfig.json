{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "vitest"
    ]
  },
  "include": [
    "src/**/*",
    "tests/js/**/*",
    "data/**/*.json"
  ],
  "version": "0.0.0"
}
