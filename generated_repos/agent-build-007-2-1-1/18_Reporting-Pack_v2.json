{
  "compliance_report": {
    "audience": [
      "Audit",
      "CAIO",
      "Compliance",
      "Legal"
    ],
    "contract": {
      "fields_min": [
        "actions_due",
        "audit_findings",
        "exceptions_open",
        "incidents_summary",
        "owners",
        "policies_changed"
      ],
      "max_tokens": 800
    },
    "gates_reference": "14_KPI+Evaluation-Framework_v2.json#gates",
    "id": "Compliance_Governance_Update_v2",
    "incident_source": "episodic:eval/*",
    "notes": "Summarize exceptions & incidents; map to governance controls; reference evidence paths."
  },
  "data_pack": {
    "contract": {
      "artifact_types": [
        "eval_results_jsonl",
        "exec_brief_md",
        "footprints_jsonl",
        "kpi_csv",
        "telemetry_parquet"
      ],
      "fields_min": [
        "artifacts",
        "datasets",
        "hashes",
        "owner",
        "pii_flags",
        "retention"
      ]
    },
    "delivery": {
      "formats": [
        "csv",
        "jsonl",
        "md",
        "parquet"
      ],
      "path_hint": "08_reports_templates/data_packs/*"
    },
    "id": "Data_Pack_Spec_v2",
    "retention": {
      "days": 365,
      "jurisdiction_overrides_ref": "05_Safety+Privacy_Guardrails_v2.json#jurisdictional_rules"
    }
  },
  "definition_of_done": [
    "Compliance report maps incidents/exceptions to gates and controls",
    "Data pack spec enumerates artifacts, retention, and formats",
    "Executive brief template available and token-light",
    "KPI views align to Observability dashboards and Evaluator cert report"
  ],
  "exec_brief": {
    "audience": [
      "Business units",
      "Executive leadership"
    ],
    "contract": {
      "fields_min": [
        "highlights",
        "kpi_deltas",
        "next_actions",
        "risks",
        "status"
      ],
      "max_tokens": 600,
      "status_enum": [
        "at_risk",
        "blocked",
        "on_track"
      ]
    },
    "integrity_inputs": {
      "fields": [
        "integrity_snapshot.status",
        "integrity_snapshot.packs_complete",
        "integrity_snapshot.objectives.status",
        "integrity_snapshot.objectives.count",
        "integrity_snapshot.objectives.has_generic",
        "integrity_snapshot.objectives.warnings"
      ],
      "notes": "Integrity snapshot should include only status/count metadata; omit raw objectives text from telemetry sinks."
    },
    "integrity_snapshot": {
      "status": "ok",
      "packs_complete": true,
      "errors": [],
      "warnings": [],
      "objectives": {
        "status": "derived",
        "count": 0,
        "has_generic": false,
        "warnings": []
      }
    },
    "id": "Executive_Summary_v2",
    "kpi_rollup": [
      "ADR",
      "AUD",
      "CST",
      "HAL",
      "INC",
      "LAT_p95",
      "PRI"
    ],
    "notes": "Token-light summary; link to data packs and dashboards; cite concisely when non-trivial."
  },
  "kpi_views": {
    "dashboards": {
      "exec_overview": [
        "ADR",
        "AUD",
        "CST",
        "HAL",
        "INC",
        "LAT_p95",
        "PRI"
      ],
      "reasoning_quality": [
        "CONA",
        "CORD",
        "RFG",
        "RTI"
      ],
      "release_cert": [
        "exceptions_open",
        "gates_passed",
        "incidents_S2plus"
      ]
    },
    "report_bindings": {
      "cert_report": {
        "fields": [
          "decision",
          "gates_outcome",
          "incidents",
          "kpi_deltas",
          "suite_results"
        ],
        "owner": "Evaluator",
        "source": "episodic:eval/*"
      }
    }
  },
  "strategy_performance": {
    "purpose": "Summarize strategy KPIs and safety posture.",
    "source_fields": {
      "reasoning_footprints": [
        "runtime_metrics.strategy_profile",
        "runtime_metrics.pri_score",
        "runtime_metrics.hal_score",
        "runtime_metrics.aud_score",
        "runtime_metrics.token_count",
        "runtime_metrics.latency_ms",
        "runtime_metrics.scenario_id"
      ],
      "telemetry_strategy_invocation": [
        "strategy_profile",
        "token_count",
        "latency_ms",
        "safety_flags",
        "scenario_id"
      ]
    }
  },
  "meta": {
    "agent_id": "AGT-112330-TURKEY-RND-0001",
    "authors": [
      "CAIO",
      "CPA",
      "TeamLead"
    ],
    "created_at": "1970-01-01T00:00:00Z",
    "name": "18_Reporting-Pack_v2.json",
    "owner": "CAIO+PMO",
    "version": "v2.0.0"
  },
  "objective": "Executive briefs, compliance updates, data pack spec, and KPI dashboard views for GEN2.",
  "outputs": [
    "docx",
    "html",
    "pdf"
  ],
  "publishing": {
    "channels": [
      "email_internal",
      "internal_sharepoint"
    ],
    "classification": "confidential"
  },
  "refs": {
    "governance": "04_Governance+Risk-Register_v2.json",
    "kpi_framework": "14_KPI+Evaluation-Framework_v2.json",
    "lifecycle": "17_Lifecycle-Pack_v2.json",
    "observability": "15_Observability+Telemetry_Spec_v2.json",
    "role_index": "06_Role-Recipes_Index_v2.json"
  },
  "schedule": {
    "on_demand": true,
    "weekly": true
  },
  "schema_keys": [
    "compliance_report",
    "data_pack",
    "definition_of_done",
    "exec_brief",
    "kpi_views",
    "strategy_performance",
    "meta",
    "objective",
    "outputs",
    "publishing",
    "refs",
    "schedule",
    "templates",
    "token_budget",
    "version"
  ],
  "templates": [
    {
      "fields": [
        "band_used",
        "confidence",
        "cost_elapsed",
        "escalation_flag",
        "escalation_reason",
        "persistence_level",
        "risk",
        "step_id",
        "time_elapsed"
      ],
      "id": "default",
      "name": "Default Template"
    }
  ],
  "token_budget": {
    "max_output_tokens": 1000
  },
  "version": 2
}
