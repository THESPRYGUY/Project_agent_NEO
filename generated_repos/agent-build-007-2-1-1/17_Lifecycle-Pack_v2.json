    "checklists": [
      {
        "id": "prelaunch",
        "title": "Pre-Launch Gate",
        "owner": "COO/PMO",
        "steps": [
          "Confirm all go_live.blockers cleared",
          "Verify approvals matrix signatures recorded",
          "Ensure evaluation evidence captured for PRI/HAL/AUD",
